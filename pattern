package javaprogram;
import java.util.Scanner;
public class Pattern {

	public static void main(String[] args) {
	Scanner sc=new Scanner(System.in);
	/*
	 * * * * *

     * * * *

     * * *

     * *

     *
	 */
	
	int num =5;
	for(int i=num;i>0;i--) {
		for(int j=1;j<=i;j++) {
			System.out.print("*");
		}
		System.out.println();
	}

	}

}
